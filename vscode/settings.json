{
    "editor.renderWhitespace": "all",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.tokenColorCustomizations": {
        "comments": "#57975a", // 注释
        "keywords": "#42d1ca", // 关键字
        "variables": "#8EE5EE", // 变量名
        "strings": "#e2d75dbd", // 字符串
        "functions": "#5b99fcc9", // 函数名
        "numbers": "#ddc850" // 数字
    },
    "workbench.colorCustomizations": {
        // "editor.selectionBackground": "#acb3b1", //Currently SELECTED text
        "editor.selectionBackground": "#e788ff7c", //Currently SELECTED text
        "editor.findMatchBackground": "#4542f7a8", //Current SEARCH MATCH
        "editor.findMatchHighlightBackground": "#ff7b00a1", //Other SEARCH MATCHES
    },
    "azure.cloud": "AzureChinaCloud",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "C_Cpp.updateChannel": "Insiders",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.autoComplete.extraPaths": [
        "/Library/Frameworks/Python.framework/Versions/3.7/lib/python3.7/site-packages"
    ],
    "remote.SSH.showLoginTerminal": true,
    "python.jediEnabled": true,
    "python.languageServer": "Jedi",
    "python.analysis.extraPaths": [
        "/Library/Frameworks/Python.framework/Versions/3.7/lib/python3.7/site-packages"
    ],
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript"
    },
    "emmet.includeLanguages": {
        "wxml": "html"
    },
    "minapp-vscode.disableAutoConfig": true,
    "http.proxySupport": "off",
    "kite.showWelcomeNotificationOnStartup": false,
    "hediet.vscode-drawio.local-storage": "",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "window.zoomLevel": 1
}